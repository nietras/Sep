{
  "Exclude": [
    "node_modules",
    ".git",
    ".vs",
    "bin",
    "obj",
    "*.png",
    "*.jpg",
    "*.jpeg",
    "*.gif",
    "*.ico",
    "*.svg",
    "*.woff",
    "*.woff2",
    "*.ttf",
    "*.eot",
    "*.min.js",
    "*.min.css",
    "package-lock.json",
    "*.lock"
  ],
  "AllowedContentTypes": [
    "text/plain",
    "application/json",
    "text/markdown"
  ],
  "PassedFiles": [],
  "Disable": {
    "EndOfLine": false,
    "Indentation": false,
    "InsertFinalNewline": false,
    "TrimTrailingWhitespace": false,
    "IndentSize": false,
    "MaxLineLength": false
  }
}
